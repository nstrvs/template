    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "gsap": "^3.9.1",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
